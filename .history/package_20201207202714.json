{
  "name": "barnwellfamilytreessg",
  "version": "1.0.0",
  "description": "family tree website",
  "main": "src/index.njk",
  "author": "peter tumulty",
  "license": "MIT",
  "scripts": {
    "build": "npm-run-all build:css build:html",
    "build:css": "postcss src/css/tailwind.css -o src/css/styles.css",
    "build:html": "eleventy",
    "watch": "npm-run-all --parallel watch:css watch:html",
    "watch:css": "postcss src/css/tailwind.css -o src/css/styles.css --watch",
    "watch:html": "eleventy --serve"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "autoprefixer": "^10.0.4",
    "axios": "^0.21.0",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.14",
    "postcss-cli": "^8.3.0",
    "tailwindcss": "^2.0.1"
  }
}
